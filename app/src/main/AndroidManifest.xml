<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.agronutritions.shop">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.agronutritions.shop.app.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_agro_logo1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_agro_logo1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.agronutritions.shop.ui.activities.AuthenticationActivity"

            android:theme="@style/AppTheme.NoActionBar"
            />
        <activity
            android:name="com.agronutritions.shop.ui.activities.Home"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.agronutritions.shop.ui.activities.MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name="com.agronutritions.shop.ui.activities.BaseActivity" />
        <activity
            android:name="com.agronutritions.shop.ui.activities.Splash"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.razorpay.ApiKey"
            android:value="rzp_live_5iLC8HI1loQDHh" />
    </application>
